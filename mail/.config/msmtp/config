account forgamesonly684@gmail.com
host smtp.gmail.com
port 587
from forgamesonly684@gmail.com
user forgamesonly684@gmail.com
passwordeval "pass forgamesonly684@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile ~/.local/share/msmtp.log
# tls_starttls

